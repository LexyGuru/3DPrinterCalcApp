{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "3DPrinterCalcApp",
  "version": "1.3.12",
  "identifier": "com.lekszikov.3dprintercalcapp",
  "build": {
    "frontendDist": "../frontend/dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "../run-frontend-dev.sh",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "3DPrinterCalcApp",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "10.13",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    }
  }
}
