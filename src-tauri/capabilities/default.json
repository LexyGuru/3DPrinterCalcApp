{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:webview:allow-print",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "dialog:allow-save",
    "dialog:allow-open",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file"
  ]
}
